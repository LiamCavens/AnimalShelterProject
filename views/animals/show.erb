<div class="show-table">
<div class="title-box">
    <div class="empty-green-box"> </div>
        <h1 class="title"> <%= @animal.name %> The <%= @animal.species %> </h1>
    <div class="empty-green-box"> </div>
</div>
        <p> Breed : <%= @animal.breed %> </p>
        <p> Age : <%= @animal.age %> </p>
        <p> Adoptable : <%= @animal.adoptable %> </p>
        <p> Admission Date : <%= @animal.admission_date %> </p>

<p>
<form action="/animals/<%= @animal.id %>/edit" method="get">
<input type="submit" value="Edit Animal">
</form>
</p>

<p>
<form action="/animals/<%= @animal.id %>/delete" method="post">
<input type="submit" value="Delete">
</form>
</p>
</div>
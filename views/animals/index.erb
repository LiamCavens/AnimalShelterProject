<div class="title-box">
    <div class="empty-green-box"> </div>
        <h1 class="title"> Animals </h1>
    <div class="empty-green-box"> </div>
</div>


<button>
    <a href="/animals/new">Add Animal</a>
</button>

<table>
<tr>
    <th>Name</th>
    <th>Species</th>
    <th>Breed</th>
    <th>Age</th>
    <th>Adoptable</th>
    <th>Admission Date</th>
</tr>
<% for animal in @animals %>
<tr>
    <td><a href="/animals/<%= animal.id %>"><%= animal.name %></a></td>
    <td><%= animal.species %></td>
    <td><%= animal.breed %></td>
    <td><%= animal.age %></td>
    <td><%= animal.adoptable %></td>
    <td><%= animal.admission_date %></td>  
</tr>
<% end %>
 </table>